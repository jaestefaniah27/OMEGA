{
    "compilerOptions": {
        "jsx": "react-native",
        "module": "commonjs",
        "target": "esnext",
        "moduleResolution": "node",
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "strict": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true
    },
    "exclude": [
        "node_modules"
    ]
}